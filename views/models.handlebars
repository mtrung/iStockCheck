<div>
<section>
  <span class="mdl-card__title-text">{{resultArray.length}} items</span>
</section>
<section id="jira">  
	<table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
		<thead>
			<tr>
				{{#each headers}}
				<th class="mdl-data-table__cell--non-numeric">{{this}}</th>
				{{/each}}				
			</tr>
		</thead>
		<tbody>
			{{#each resultArray}}
			<tr class="main_row">
				<td class="mdl-data-table__cell--non-numeric">{{modelCode}}</td>
				<td class="mdl-data-table__cell--non-numeric">{{modelName}}</td>
				<td class="mdl-data-table__cell--non-numeric">{{color}}</td>
				<td class="mdl-data-table__cell--non-numeric">{{storage}}</td>
				<td class="mdl-data-table__cell--non-numeric">{{carrier}}</td>
				<td class="mdl-data-table__cell--non-numeric">{{display}}</td>
			</tr>
			{{/each}}
		</tbody>
	</table>
</section>
</div>
